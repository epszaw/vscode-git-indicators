{
  "name": "git-indicators",
  "displayName": "Git Indicators",
  "description": "Atom like git counters",
  "version": "1.2.5",
  "publisher": "lamartire",
  "repository": {
    "type": "git",
    "url": "https://github.com/lamartire/vscode-git-indicators"
  },
  "homepage": "https://github.com/lamartire/vscode-git-indicators",
  "engines": {
    "vscode": "^1.10.0"
  },
  "icon": "icon.jpg",
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "git-indicators.initIndicators",
        "title": "Git indicators: activate"
      }
    ]
  },
  "activationEvents": [
    "workspaceContains:.git"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.50",
    "mocha": "^4.0.1",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  }
}
