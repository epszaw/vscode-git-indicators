{
  "name": "git-indicators",
  "displayName": "Git Indicators",
  "description": "Atom like git counters",
  "version": "2.1.1",
  "publisher": "lamartire",
  "repository": {
    "type": "git",
    "url": "https://github.com/lamartire/vscode-git-indicators"
  },
  "homepage": "https://github.com/lamartire/vscode-git-indicators",
  "engines": {
    "vscode": "^1.10.0"
  },
  "icon": "icon.jpg",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:.git"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.4",
    "@types/throttle-debounce": "^1.0.0",
    "mocha": "^5.0.1",
    "prettier": "^1.10.2",
    "tslint": "^5.9.1",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.0.1",
    "vscode": "^1.1.10"
  },
  "dependencies": {
    "promisify-node": "^0.5.0",
    "throttle-debounce": "^2.0.1",
    "typed-promisify": "^0.4.0"
  }
}
