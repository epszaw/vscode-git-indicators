{
  "name": "git-indicators",
  "displayName": "Git Indicators",
  "description": "Atom like git counters",
  "version": "0.0.6",
  "publisher": "lamartire",
  "repository": {
    "type": "git",
    "url": "https://github.com/lamartire/vscode-git-indicators"
  },
  "homepage": "https://github.com/lamartire/vscode-git-indicators",
  "engines": {
    "vscode": "^1.10.0"
  },
  "icon": "icon.jpg",
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "git-indicators.initIndicators",
        "title": "Git indicators: activate"
      },
      {
        "command": "git-indicators.updateIndicators",
        "title": "Git indicators: update indicators data"
      }
    ]
  },
  "activationEvents": [
    "workspaceContains:.git"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "bluebird": "^3.5.0"
  }
}
