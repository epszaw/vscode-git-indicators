{
  "name": "git-indicators",
  "displayName": "Git Indicators",
  "description": "Atom like git counters",
  "version": "2.1.0",
  "publisher": "lamartire",
  "repository": {
    "type": "git",
    "url": "https://github.com/lamartire/vscode-git-indicators"
  },
  "homepage": "https://github.com/lamartire/vscode-git-indicators",
  "engines": {
    "vscode": "^1.10.0"
  },
  "icon": "icon.jpg",
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "git-indicators.initIndicators",
        "title": "Git indicators: activate"
      }
    ]
  },
  "activationEvents": [
    "workspaceContains:.git"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "@types/throttle-debounce": "^1.0.0",
    "mocha": "^5.0.1",
    "prettier": "^1.10.2",
    "tslint": "^5.9.1",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.7.2",
    "vscode": "^1.1.10"
  },
  "dependencies": {
    "promisify-node": "^0.4.0",
    "throttle-debounce": "^1.0.1",
    "typed-promisify": "^0.4.0"
  }
}
